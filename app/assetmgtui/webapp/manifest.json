{
  "_version": "1.59.0",
  "sap.app": {
    "id": "assetmgtui",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.12.4",
      "toolsId": "38ce166f-ca4c-40bc-a0cb-bab7fd16edb0"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/catalog/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.120.7",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.suite.ui.generic.template": {},
        "sap.ui.comp": {},
        "sap.ui.generic.app": {},
        "sap.ui.table": {},
        "sap.ushell": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "assetmgtui.i18n.i18n"
        }
      },
      "dataModel":{
        "uri":"model/dataModel.json",
        "type":"sap.ui.model.json.JSONModel"
      },
      "locModel":{
        "uri":"model/ProcessFlowLanesAndNodes.json",
        "type":"sap.ui.model.json.JSONModel"
      },
      "oDaModel": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "synchronizationMode": "None",
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }

    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "assetmgtui.view",
        "controlAggregation": "pages",
        "controlId": "app",
        "clearControlAggregation": false
      },
      "routes": [
        {
          "name": "RouteHR",
          "pattern": ":?query:",
          "target": [
            "TargetHR"
          ]
        },
        {
          "name": "TL1Home",
          "pattern": "TL1Home",
          "target": ["TargetTL1Home"]
        },
        {
          "name": "TL2Home",
          "pattern": "TL2Home",
          "target": ["TargetTL2Home"]
        },
        {
          "name": "TL1RaisedRequests",
          "pattern": "TL1RaisedRequests",
          "target": ["TargetTL1RaisedRequests"]
        },
        {
          "name": "TL1FinanceResponces",
          "pattern": "TL1FinanceResponces",
          "target": ["TargetTL1FinanceResponces"]
        },
        {
          "name": "TL1StatusUpdatedRequests",
          "pattern": "TL1StatusUpdatedRequests",
          "target": ["TargetTL1StatusUpdatedRequests"]
        }
      ],
      "targets": {
        "TargetHR": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "HR",
          "viewName": "HR"
        },
        "TargetTL1Home": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TL1Home",
          "viewName": "TL1Home"
        },
        "TargetTL2Home": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TL2Home",
          "viewName": "TL2Home"
        },
        
        "TargetTL1RaisedRequests": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TL1RaisedRequests",
          "viewName": "TL1RaisedRequests"
        },
        "TargetTL1FinanceResponces": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TL1FinanceResponces",
          "viewName": "TL1FinanceResponces"
        },
        "TargetTL1StatusUpdatedRequests": {
          "viewType": "XML",
          "transition": "slide",
          "clearControlAggregation": false,
          "viewId": "TL1StatusUpdatedRequests",
          "viewName": "TL1StatusUpdatedRequests"
        }
      }
    },
    "rootView": {
      "viewName": "assetmgtui.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    }
  }
}